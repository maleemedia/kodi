        provider-name="MaleeMedia">
    <requires>
        <import addon="xbmc.addon" version="17.0.0"/>
    </requires>
    <extension point="xbmc.addon.repository"
        name="MaleeMedia Repo">
        <dir minversion="17.0.0">
            <info compressed="false">https://raw.githubusercontent.com/maleemedia/kodi/master/Krypton/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/maleemedia/kodi/master/Krypton/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/maleemedia/kodi/master/Krypton/</datadir>
        </dir>
        <dir minversion="18.0.0">
            <info compressed="false">https://raw.githubusercontent.com/maleemedia/kodi/master/Leia/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/maleemedia/kodi/master/Leia/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/maleemedia/kodi/master/Leia/</datadir>
